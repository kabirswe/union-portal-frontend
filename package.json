{
  "name": "union-portal-frontend",
  "version": "0.0.0",
  "description": "UNION Portal Frontend",
  "author": "JP inc",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=14.0.0 <15.0.0",
    "yarn": "^1.22.10"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.(ts|tsx)": [
      "eslint --color",
      "prettier --check"
    ],
    "src/**/*.scss": [
      "stylelint",
      "prettier --check"
    ]
  },
  "dependencies": {
    "@hookform/devtools": "^3.1.0",
    "@reduxjs/toolkit": "^1.6.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "axios": "^0.21.1",
    "change-case": "^4.1.2",
    "choices.js": "^9.0.1",
    "ckeditor4-react": "^2.0.0-rc.2",
    "date-fns": "^2.22.1",
    "dayjs": "^1.10.6",
    "escape-string-regexp": "^5.0.0",
    "event-target-polyfill": "^0.0.3",
    "exenv": "^1.2.2",
    "flatpickr": "^4.6.9",
    "gsap": "^3.7.0",
    "immutability-helper": "^3.1.1",
    "lodash": "^4.17.21",
    "node-sass": "4.14.1",
    "quill": "^1.3.7",
    "quill-mention": "^3.0.3",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.12.2",
    "react-query": "^3.18.1",
    "react-redux": "^7.2.4",
    "react-responsive": "^9.0.0-beta.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-toastify": "^7.0.4",
    "regenerator-runtime": "^0.13.7",
    "remove": "^0.1.5",
    "sanitize.css": "^12.0.1",
    "sass": "^1.35.1",
    "typescript": "^4.1.2",
    "viewport-extra": "^2.1.1",
    "web-vitals": "^1.0.1",
    "yup": "0.32.9"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "gen:component:atom": "cross-env HYGEN_TMPLS=scaffolds hygen component new --level atom",
    "gen:component:molecule": "cross-env HYGEN_TMPLS=scaffolds hygen component new --level molecule",
    "gen:component:organism": "cross-env HYGEN_TMPLS=scaffolds hygen component new --level organism",
    "gen:component:utils": "cross-env HYGEN_TMPLS=scaffolds hygen component new --level utils",
    "gen:api": "run-s gen:api:cleanup gen:api:orval gen:api:postgenerate",
    "gen:api:cleanup": "rimraf ./src/api/model* ./src/api/api*.ts",
    "gen:api:orval": "orval --config ./orval.config.js",
    "gen:api:postgenerate": "cross-env HYGEN_TMPLS=scaffolds hygen api postgenerate",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "run-s lint:type lint:ts lint:style prettier-write",
    "lint:ts": "eslint ./src --ext .ts --ext .tsx",
    "lint:style": "stylelint \"./src/**/*.{css,scss}\"",
    "lint:type": "tsc -p ./tsconfig.json",
    "prettier-check": "prettier --check \"src/**/*.@(ts|tsx|scss)\"",
    "prettier-check:script": "prettier --check \"src/**/*.@(ts|tsx)\"",
    "prettier-check:style": "prettier --check \"src/**/*.@(scss)\"",
    "prettier-write": "prettier --write \"src/**/*.@(ts|tsx|scss)\"",
    "prettier-write:script": "prettier --write \"src/**/*.@(ts|tsx)\"",
    "prettier-write:style": "prettier --write \"src/**/*.@(scss)\"",
    "postinstall": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@hookform/resolvers": "2.7.0",
    "@juggle/resize-observer": "^3.3.1",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/node-logger": "^6.2.9",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.9",
    "@types/ckeditor": "^4.9.10",
    "@types/exenv": "^1.2.0",
    "@types/faker": "^5.5.7",
    "@types/lodash": "^4.14.170",
    "@types/quill": "^2.0.8",
    "@types/react-beautiful-dnd": "^13.1.1",
    "@types/react-flatpickr": "^3.8.3",
    "@types/react-router-dom": "^5.1.7",
    "@types/simplebar": "^5.3.3",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@types/yup": "0.29.13",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "@whitespace/storybook-addon-html": "^5.0.0",
    "core-js": "^3.15.1",
    "cross-env": "^7.0.3",
    "detect-browser": "^5.2.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "faker": "^5.5.3",
    "husky": "^6.0.0",
    "hygen": "^6.1.0",
    "lint-staged": "^11.0.0",
    "msw": "^0.32.0",
    "npm-run-all": "^4.1.5",
    "orval": "^5.4.14",
    "path": "^0.12.7",
    "prettier": "^2.3.1",
    "react-app-rewired": "^2.1.8",
    "rimraf": "^3.0.2",
    "sass-resources-loader": "^2.2.2",
    "simplebar": "^5.3.4",
    "smoothscroll-polyfill": "^0.4.4",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.4.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.19.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
